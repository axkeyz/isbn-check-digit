{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/vue3-essential",
        "google",
        "eslint:recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "ignorePatterns": ["/*", "!/app", "!/app/frontend"],
    "rules": {
        "max-len": ["error", { "code": 100 }],
        "linebreak-style": 0
    }
}
